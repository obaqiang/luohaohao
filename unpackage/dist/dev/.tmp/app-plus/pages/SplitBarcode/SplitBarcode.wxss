	        justify-content: center;
	margin: 0 50rpx;
	padding: 0 5rpx;
}
.pic_input_in{
	padding: 0;
}


.SplitBarcode {
	padding-bottom: 200rpx;
}
.btn_bot {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	position: fixed;
	bottom: 40rpx;
	width: 750rpx;
}
.btn_bot button {
	background: #FFB700;
	font-size: 30rpx;
	padding: 5rpx 120rpx;
}
.btn_bot button:nth-child(1) {
	background: #FFB700;
	color: #FFFFFF;
}
.btn_bot button:nth-child(2) {
	background: #FFB700;
	color: #666666;
}
